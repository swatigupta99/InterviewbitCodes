int Solution::solve(vector<int> &A, int B) {
    int n= A.size();
    int count =0;
    int start = 0 ,end = A.size()-1;
    
    while (start <= end)
    {
        int mid = (start + end)/2;
        if (A[mid] <= B ){
            count = mid+1;
            start = mid +1;
           
        }
        else 
        {
            end = mid - 1; 
        }
    }
    return count;
}
