int Solution::searchMatrix(vector<vector<int> > &A, int B) {
    int r = A.size();
    int c = A[0].size();
    for (int i =0; i < r;i++)
    {
        for (int j =0; j< c;j++)
        {
            if (A[i][j]==B)
            {
                return 1;
            }
        }
    }
    return 0;
}

